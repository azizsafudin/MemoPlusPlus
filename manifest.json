{
  "manifest_version": 2,
 
  "name": "Memo++",
  "version": "0.4.1",
  "description": "A memo.cash extension",
  "icons": 
  {
    "16": "ext_16.png",
    "48": "ext_48.png",
    "128": "ext_128.png" 
  },
  "permissions": [
    "https://publish.twitter.com/*",
    "https://api.instagram.com/*"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery.min.js", "content.js", "favico.min.js"],
    "matches": ["https://memo.cash/*"],
    "run_at": "document_end"
  }]
 
}