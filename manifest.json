{
  "manifest_version": 2,
 
  "name": "Memo++",
  "version": "0.2.0",
  "description": "Memo.cash extension",
 
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery.min.js", "content.js", "favico.min.js"],
    "matches": ["https://memo.cash/*"],
    "run_at": "document_end"
  }]
 
}